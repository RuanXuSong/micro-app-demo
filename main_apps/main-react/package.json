{
  "name": "ant-design-pro",
  "version": "4.5.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 max build",
    "build": "max build  && rm -rf ../../build && mv build ../../",
    "deploy": "npm run site && npm run gh-pages",
    "dev": "npm run start:dev",
    "fetch:blocks": "pro fetch-blocks && npm run prettier",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "max setup",
    "lint": "max setup && npm run lint:js && npm run lint:prettier",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "start": "cross-env UMI_ENV=dev max dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev max dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev max dev",
    "start:no-ui": "cross-env UMI_UI=none UMI_ENV=dev max dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev max dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev max dev",
    "pretest": "node ./tests/beforeTest",
    "test": "max test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "max test ./src/components"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@alita/plugins": "https://registry.yarnpkg.com/@alita/plugins/-/plugins-3.0.20.tgz#0f4787012a94dea5f3e64740281f78ed3708ddfc",
    "@ant-design/icons": "^4.0.0",
    "@ant-design/pro-descriptions": "^1.2.0",
    "@ant-design/pro-form": "^1.3.0",
    "@ant-design/pro-layout": "^6.9.0",
    "@ant-design/pro-table": "^2.17.0",
    "@micro-zoe/micro-app": "0.8.7",
    "@micro-zoe/plugin-painful-joya": "~1.0.0",
    "@umijs/max": "^4.0.0",
    "@umijs/plugin-utils": "^1.1.0",
    "@umijs/route-utils": "^1.0.33",
    "ahooks": "2.4.0",
    "babel-polyfill": "~6.26.0",
    "classnames": "^2.2.6",
    "darkreader": "^4.9.57",
    "immer": "^7.0.7",
    "lodash": "^4.17.11",
    "moment": "^2.25.3",
    "qs": "^6.9.0",
    "rc-util": "5.22.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet-async": "^1.0.4",
    "react-loadable": "^5.5.0",
    "react-markdown": "4.3.1",
    "umi-request": "^1.0.8",
    "use-immer": "^0.4.1"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^1.0.28",
    "@types/classnames": "^2.2.7",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.2",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.144",
    "@types/lodash-es": "^4.17.6",
    "@types/qs": "^6.5.3",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.1",
    "@types/react-helmet": "^6.1.0",
    "@types/react-loadable": "^5.5.6",
    "@umijs/fabric": "^2.5.1",
    "@umijs/yorkie": "^2.0.3",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.1.1",
    "detect-installer": "^1.0.1",
    "enzyme": "^3.11.0",
    "eslint": "^7.1.0",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "gh-pages": "^3.0.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.0.0",
    "mockjs": "^1.0.1-beta3",
    "openbrowser": "~1.0.2",
    "prettier": "^2.0.1",
    "react-dev-utils": "~11.0.4",
    "stylelint": "^13.0.0",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
